{
  "private": true,
  "scripts": {
    "prod:css": "NODE_ENV=production npx postcss _assets/css -d assets/css",
    "dev:css": "NODE_ENV=development npx postcss _assets/css -d assets/css",
    "concat:js": "mkdir -p assets/js && npx concat -o assets/js/index_pre.js _assets/js/*.js",
    "build:js": "npx babel -o assets/js/index.js assets/js/index_pre.js",
    "prod:js": "npx uglifyjs --compress --mangle -o assets/js/index.js -- assets/js/index.js",
    "prod": "npm run prod:css && npm run concat:js && npm run build:js && rm assets/js/index_pre.js && npm run prod:js",
    "dev": "npm run dev:css && npm run concat:js && npm run build:js"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.7.2",
    "concat": "^1.0.3",
    "cssnano": "^4.1.10",
    "postcss": "^7.0.23",
    "postcss-cli": "^6.1.3",
    "postcss-nested": "^4.2.1",
    "tailwindcss": "^1.1.4",
    "uglify-js": "^3.7.0"
  },
  "dependencies": {}
}
